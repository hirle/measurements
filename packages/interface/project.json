{
  "root": "packages/interface",
  "sourceRoot": "packages/interface/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/interface",
        "main": "packages/interface/src/index.ts",
        "tsConfig": "packages/interface/tsconfig.lib.json",
        "assets": ["packages/interface/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/interface/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/interface"],
      "options": {
        "jestConfig": "packages/interface/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
